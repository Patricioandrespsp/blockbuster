<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movies.last %></td>
    <td><%= link_to "Ver cliente", client %></td>
    <td><%= link_to "Editar datos de cliente", edit_client_path(client) %></td>
    <td><%= button_to "Eliminar cliente", @client, method: :delete, class: "btn btn-danger btn-sm" %></td>
  </tr>
</div>
